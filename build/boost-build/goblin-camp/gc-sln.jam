# Generators for Visual Studio project files.

import modules ;

user-config = [ modules.peek : SLN_USE_CONFIG ] ;

actions generate-sln {
    python "$(GC_TEMPLATES)/genvcproj.py" sln "$(GC_SOURCE_ROOT)" "$(>)" "$(<)" "$(user-config)"
}

actions generate-2008 {
    python "$(GC_TEMPLATES)/genvcproj.py" 2008 "$(GC_SOURCE_ROOT)" "$(>)" "$(<)" "$(user-config)"
}

actions generate-2010 {
    python "$(GC_TEMPLATES)/genvcproj.py" 2010 "$(GC_SOURCE_ROOT)" "$(>)" "$(<)" "$(user-config)"
}
